<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="DAL.LoanType" table="loantype">
        <id name="loanTypeId" type="java.lang.Integer">
            <column name="id" sql-type="Integer"></column>
            <generator class="increment"></generator>
        </id>
        <property name="loanName" type="String">
            <column name="loanName" sql-type="" not-null="true"/>
        </property>
        <property name="interestRate" type="java.lang.Float">
            <column name="interestrate" sql-type="Integer" not-null="true"/>
        </property>
        <list name="grantConditions">
            <key></key>
            <index></index>
            <one-to-many></one-to-many>
        </list>
        <list name="loanFiles">
            <key></key>
            <index></index>
            <one-to-many></one-to-many>
        </list>
    </class>
</hibernate-mapping>